#include<iostream>
using namespace std;
int main(){
	int num1,num2,num3;
	cout<<"Largest of three numbers"<<endl;
	cout<<"Enter three numbers:"<<endl;
	cin>>num1>>num2>>num3;
	if(num1>=num2 && num1>=num3){
	cout<<"Largest of three numbers is "<<num1<<endl;

	}
	if(num2>=num3 && num2>=num1){
	cout<<"Largest of three numbers is "<<num2<<endl;

	}
	else{
	cout<<"Largest of three numbers is "<<num3<<endl;

	}
	return 0;
}